
public class Biglietteria {
	private String codice;
	private String nome;
	private String compagnia1;
	private String compagnia2;
	
	public Biglietteria(String codice, String nome, String compagnia1, String compagnia2) {
		super();
		this.codice = codice;
		this.nome = nome;
		this.compagnia1 = compagnia1;
		this.compagnia2 = compagnia2;
	}

	public String getCodice() {
		return codice;
	}

	public void setCodice(String codice) {
		this.codice = codice;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getCompagnia1() {
		return compagnia1;
	}

	public void setCompagnia1(String compagnia1) {
		this.compagnia1 = compagnia1;
	}

	public String getCompagnia2() {
		return compagnia2;
	}

	public void setCompagnia2(String compagnia2) {
		this.compagnia2 = compagnia2;
	}

	@Override
	public String toString() {
		return "Biglietteria [codice=" + codice + ", nome=" + nome + ", compagnia1=" + compagnia1 + ", compagnia2="
				+ compagnia2 + "]";
	}
	
	public void prenotaPosto(String codice)
	
}
